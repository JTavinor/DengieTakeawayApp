/configureStore\";\nimport { Provider } from \"react-redux\";\nimport Footer from \"./components/common/Footer\";\n\nconst store = configureStore();\n\nfunction App() {\n  return (\n    <>\n      <Provider store={store}>\n        <Navbar />\n        <Switch>\n          <Route path=\"/menu/:name\" render={(props) => <Menu {...props} />} />\n\n          <Route path=\"/\">\n            <CuisineList cuisines={restaurants} />\n          </Route>\n        </Switch>\n        <Footer />\n      </Provider>\n    </>\n  );\n}\n\nexport default App;\n","/Users/josephtavinor/DengieTakeawayApp/DengieTakeawayApp/dengie-takeaway-app/src/reportWebVitals.js",[],"/Users/josephtavinor/DengieTakeawayApp/DengieTakeawayApp/dengie-takeaway-app/src/components/ScrollToTop.jsx",[],"/Users/josephtavinor/DengieTakeawayApp/DengieTakeawayApp/dengie-takeaway-app/src/data/cuisines.js",[],"/Users/josephtavinor/DengieTakeawayApp/DengieTakeawayApp/dengie-takeaway-app/src/components/common/Navbar.jsx",[],"/Users/josephtavinor/DengieTakeawayApp/DengieTakeawayApp/dengie-takeaway-app/src/components/menu/Menu.jsx",["129","130"],"import React, { useEffect, useState } from \"react\";\nimport MenuCard from \"./MenuCard\";\nimport MenuLocator from \"./MenuLocator\";\nimport Basket from \"./Basket\";\nimport _ from \"lodash\";\nimport \"../../css/menu/menu.css\";\nimport axios from \"axios\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { loadMenu } from \"../../store/menus\";\nimport { Bars } from \"react-loading-icons\";\n\nfunction Menu({ location }) {\n  const dispatch = useDispatch();\n\n  const menu = useSelector((state) => state.entities.menu.list.menu);\n  const loading = useSelector((state) => state.entities.menu.loading);\n\n  useEffect(() => {\n    dispatch(loadMenu(location.state.menuId));\n  }, []);\n\n  const [basket, setBasket] = useState({ subtotal: 0 });\n\n  const addItemToBasket = ({ item, quantity, price }) => {\n    const oldTotal = basket.subtotal;\n\n    // If basket does not contain the item\n    if (!basket[item])\n      return setBasket({\n        ...basket,\n        [item]: { quantity: quantity, price: _.round(price, 2) },\n        subtotal: _.round(oldTotal + price, 2),\n      });\n\n    const oldQuantity = basket[item].quantity;\n    const oldPrice = basket[item].price;\n    // If basket already contains the item\n    return setBasket({\n      ...basket,\n      [item]: {\n        quantity: oldQuantity + quantity,\n        price: _.round(oldPrice + price, 2),\n      },\n      subtotal: _.round(oldTotal + price, 2),\n    });\n  };\n\n  return (\n    <>\n      {loading && (\n        <div className=\"loadingIcon\">\n          <Bars height=\"100px\" />\n        </div>\n      )}\n      {!loading && menu && (\n        <>\n          <div>{menu.restaurant}</div>\n          <div className=\"menuPageContainer\">\n            <div className=\"menuContainer\">\n              <div style={{ width: \"15%\" }}>\n                <MenuLocator menu={menu} />\n              </div>\n              <div className=\"menuCardContainer\">\n                <MenuCard menu={menu} addItemToBasket={addItemToBasket} />\n              </div>\n\n              <div className=\"menuBasketContainer\">\n                <Basket basket={basket} />\n              </div>\n            </div>\n          </div>\n        </>\n      )}\n    </>\n  );\n}\n\nexport default Menu;\n","/Users/josephtavinor/DengieTakeawayApp/DengieTakeawayApp/dengie-takeaway-app/src/components/homePage/cuisineList.jsx",[],["131","132"],"/Users/josephtavinor/DengieTakeawayApp/DengieTakeawayApp/dengie-takeaway-app/src/components/menu/MenuCard.jsx",[],"/Users/josephtavinor/DengieTakeawayApp/DengieTakeawayApp/dengie-takeaway-app/src/components/homePage/cuisineCard.jsx",[],"/Users/josephtavinor/DengieTakeawayApp/DengieTakeawayApp/dengie-takeaway-app/src/components/menu/Basket.jsx",[],"/Users/josephtavinor/DengieTakeawayApp/DengieTakeawayApp/dengie-takeaway-app/src/components/menu/MenuLocator.jsx"