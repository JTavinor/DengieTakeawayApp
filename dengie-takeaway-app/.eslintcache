sages":"132","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"72"},{"filePath":"133","messages":"134","errorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":"135","usedDeprecatedRules":"72"},{"filePath":"136","messages":"137","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"72"},{"filePath":"138","messages":"139","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"72"},{"filePath":"140","messages":"141","errorCount":0,"warningCount":4,"fixableErrorCount":0,"fixableWarningCount":0,"source":null},{"filePath":"142","messages":"143","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"72"},{"filePath":"144","messages":"145","errorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":"146","usedDeprecatedRules":"72"},{"filePath":"147","messages":"148","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"72"},"/Users/josephtavinor/DengieTakeawayApp/DengieTakeawayApp/dengie-takeaway-app/src/index.js",[],["149","150"],"/Users/josephtavinor/DengieTakeawayApp/DengieTakeawayApp/dengie-takeaway-app/src/App.js",["151"],"import \"./App.css\";\nimport { restaurants, menus } from \"./data/cuisines\";\nimport Navbar from \"./components/common/Navbar\";\nimport Menu from \"./components/menu/Menu\";\nimport CuisineList from \"./components/homePage/CuisineList\";\nimport { Route, Switch } from \"react-router-dom\";\n\nimport configureStore from \"./store/configureStore\";\nimport { Provider } from \"react-redux\";\nimport Footer from \"./components/common/Footer\";\nimport Checkout from \"./components/Checkout.jsx\";\nimport OrderDetails from \"./components/OrderDetails\";\n\nconst store = configureStore();\n\nfunction App() {\n  return (\n    <>\n      <Provider store={store}>\n        <Navbar />\n        <main>\n          <Switch>\n            <Route path=\"/order-details\">\n              <OrderDetails />\n            </Route>\n            <Route path=\"/checkout\">\n              <Checkout />\n            </Route>\n            <Route path=\"/menu/:name\" render={(props) => <Menu {...props} />} />\n\n            <Route path=\"/\">\n              <CuisineList cuisines={restaurants} />\n            </Route>\n          </Switch>\n        </main>\n        <Footer />\n      </Provider>\n    </>\n  );\n}\n\nexport default App;\n","/Users/josephtavinor/DengieTakeawayApp/DengieTakeawayApp/dengie-takeaway-app/src/reportWebVitals.js",[],"/Users/josephtavinor/DengieTakeawayApp/DengieTakeawayApp/dengie-takeaway-app/src/components/ScrollToTop.jsx",[],"/Users/josephtavinor/DengieTakeawayApp/DengieTakeawayApp/dengie-takeaway-app/src/data/cuisines.js",[],"/Users/josephtavinor/DengieTakeawayApp/DengieTakeawayApp/dengie-takeaway-app/src/components/common/Navbar.jsx",[],"/Users/josephtavinor/DengieTakeawayApp/DengieTakeawayApp/dengie-takeaway-app/src/components/menu/Menu.jsx",["152"],"import React, { useEffect, useState } from \"react\";\nimport MenuCard from \"./MenuCard\";\nimport MenuLocator from \"./MenuLocator\";\nimport Basket from \"./Basket\";\nimport _ from \"lodash\";\nimport \"../../css/menu/menu.css\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { loadMenu } from \"../../store/menus\";\nimport { Bars } from \"react-loading-icons\";\n\nfunction Menu({ location }) {\n  const dispatch = useDispatch();\n\n  const menu = useSelector((state) => state.entities.menu.list.menu);\n  const loading = useSelector((state) => state.entities.menu.loading);\n\n  useEffect(() => {\n    dispatch(loadMenu(location.state.menuId));\n  }, []);\n\n  const [basket, setBasket] = useState({ subtotal: 0 });\n\n  const addItemToBasket = ({ item, quantity, price }) => {\n    const oldTotal = basket.subtotal;\n\n    // If basket does not contain the item\n    if (!basket[item])\n      return setBasket({\n        ...basket,\n        [item]: { quantity: quantity, price: _.round(price, 2) },\n        subtotal: _.round(oldTotal + price, 2),\n      });\n\n    const oldQuantity = basket[item].quantity;\n    const oldPrice = basket[item].price;\n    // If basket already contains the item\n    return setBasket({\n      ...basket,\n      [item]: {\n        quantity: oldQuantity + quantity,\n        price: _.round(oldPrice + price, 2),\n      },\n      subtotal: _.round(oldTotal + price, 2),\n    });\n  };\n\n  return (\n    <>\n      {loading && (\n        <div className=\"loadingIcon\">\n          <Bars height=\"100px\" />\n        </div>\n      )}\n      {!loading && menu && (\n        <>\n          <div>{menu.restaurant}</div>\n          <div className=\"menuPageContainer\">\n            <div className=\"menuContainer\">\n              <div style={{ width: \"15%\" }}>\n                <MenuLocator menu={menu} />\n              </div>\n              <div className=\"menuCardContainer\">\n                <MenuCard menu={menu} addItemToBasket={addItemToBasket} />\n              </div>\n\n              <div className=\"menuBasketContainer\">\n                <Basket basket={basket} />\n              </div>\n            </div>\n          </div>\n        </>\n      )}\n    </>\n  );\n}\n\nexport default Menu;\n","/Users/josephtavinor/DengieTakeawayApp/DengieTakeawayApp/dengie-takeaway-app/src/components/homePage/cuisineList.jsx",[],["153","154"],"/Users/josephtavinor/DengieTakeawayApp/DengieTakeawayApp/dengie-takeaway-app/src/components/menu/MenuCard.jsx",[],"/Users/josephtavinor/DengieTakeawayApp/DengieTakeawayApp/dengie-takeaway-app/src/components/homePage/cuisineCard.jsx",[],"/Users/josephtavinor/DengieTakeawayApp/DengieTakeawayApp/dengie-takeaway-app/src/components/menu/Basket.jsx",["155"],"import React, { useState } from \"react\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport \"../../css/menu/basket.css\";\nimport {\n  getSubtotal,\n  itemQuantityUpdated,\n  itemRemoved,\n} from \"../../store/basket\";\nimport { FontAwesomeIcon } from \"@fortawesome/react-fontawesome\";\nimport { faTrashAlt } from \"@fortawesome/free-solid-svg-icons\";\nimport { Link } from \"react-router-dom\";\n\nconst basketContainer = {\n  backgroundColor: \"whitesmoke\",\n  border: \"1px solid gray\",\n  borderRadius: \"5px\",\n  display: \"flex\",\n  flexDirection: \"column\",\n  margin: \"auto\",\n  padding: \"15px 30px\",\n  position: \"sticky\",\n  textAlign: \"center\",\n  top: 15,\n  width: \"75%\",\n};\n\nfunction Basket() {\n  // Removing subtotal from basket\n  // const itemBasket = { ...basket };\n  // delete itemBasket.subtotal;\n\n  const dispatch = useDispatch();\n  const basket = useSelector((state) => state.entities.basket);\n  // console.log(basket);\n\n  const basketIsNotEmpty = () => {\n    return JSON.stringify(basket) !== \"{}\";\n  };\n\n  const getSubtotal = (basket) => {\n    let subTotal = 0;\n    if (basket.length === 0) return subTotal;\n    for (const item of basket) {\n      subTotal += item.price;\n    }\n    return subTotal;\n  };\n\n  const decrementItem = (item) => {\n    if (basket[item].quantity > 1) {\n      return dispatch(\n        itemQuantityUpdated({\n          itemName: basket[item].itemName,\n          quantity: -1,\n          price: basket[item].price,\n        })\n      );\n    } else {\n      dispatch(itemRemoved({ itemName: basket[item].itemName }));\n    }\n  };\n\n  const incrementItem = (item) => {\n    dispatch(\n      itemQuantityUpdated({\n        itemName: basket[item].itemName,\n        quantity: 1,\n        price: basket[item].price,\n      })\n    );\n  };\n\n  const renderBasket = () => {\n    const basketList = [];\n    for (const item in basket) {\n      basketList.push(\n        <li className=\"basketListItem\" key={item}>\n          <div style={{ display: \"flex\", flexDirection: \"row\" }}>\n            {basket[item].quantity}x {basket[item].itemName}\n            <div className=\"counterContainer\">\n              <button\n                className=\"counterButton minus smallButton\"\n                onClick={() => decrementItem(item)}\n              >\n                -\n              </button>\n              <button\n                className=\"counterButton plus smallButton\"\n                onClick={() => incrementItem(item)}\n              >\n                +\n              </button>\n            </div>\n          </div>\n          <div>Price: £{basket[item].price}</div>\n          <button\n            className=\"counterButton minus smallButton\"\n            onClick={() =>\n              dispatch(itemRemoved({ itemName: basket[item].itemName }))\n            }\n          >\n            <FontAwesomeIcon icon={faTrashAlt} size=\"xs\" />\n          </button>\n        </li>\n      );\n    }\n    return <ul className=\"basketList\">{basketList}</ul>;\n  };\n\n  const [delivery, setDelivery] = useState(\"delivery\");\n\n  return (\n    <div style={basketContainer}>\n      <div\n        style={{\n          display: \"flex\",\n          flexDirection: \"row\",\n          justifyContent: \"center\",\n        }}\n        onChange={(e) => setDelivery(e.target.value)}\n      >\n        <br />\n        <input\n          type=\"radio\"\n          id=\"delivery\"\n          name=\"gender\"\n          value=\"delivery\"\n          defaultChecked\n        />\n        <label for=\"delivery\">Delivery</label>\n        <input type=\"radio\" id=\"collection\" name=\"gender\" value=\"collection\" />\n        <label for=\"collection\">Collection</label>\n        <br />\n      </div>\n      <h2 className=\"basketHeader\">Your Order</h2>\n      <div>\n        {basketIsNotEmpty() && renderBasket()}\n        <div className=\"basketSubtotalContainer\">\n          <p className=\"basketSubtotalElement\">Subtotal</p>\n          <p className=\"basketSubtotalElement\">£{getSubtotal(basket)}</p>\n        </div>\n        <Link to={delivery === \"delivery\" ? \"/checkout\" : \"/order-details\"}>\n          <button\n            className=\"addToBasketButton\"\n            style={{\n              width: \"100%\",\n              textAlign: \"center\",\n              padding: \"10px\",\n              justifyContent: \"center\",\n            }}\n          >\n            <p style={{ margin: 0 }}>Go to Checkout</p>\n          </button>\n        </Link>\n      </div>\n    </div>\n  );\n}\n\nexport default Basket;\n","/Users/josephtavinor/DengieTakeawayApp/DengieTakeawayApp/dengie-takeaway-app/src/components/menu/MenuLocator.jsx",[],"/Users/josephtavinor/DengieTakeawayApp/DengieTakeawayApp/dengie-takeaway-app/src/components/menu/SubmenuCard.jsx",[],"/Users/josephtavinor/DengieTakeawayApp/DengieTakeawayApp/dengie-takeaway-app/src/components/homePage/restaurantList.jsx",[],"/Users/josephtavinor/DengieTakeawayApp/DengieTakeawayApp/dengie-takeaway-app/src/components/menu/ItemCard.jsx",[],"/Users/josephtavinor/DengieTakeawayApp/DengieTakeawayApp/dengie-takeaway-app/src/components/homePage/restaurantCard.jsx",[],"/Users/josephtavinor/DengieTakeawayApp/DengieTakeawayApp/dengie-takeaway-app/src/components/menu/MenuModal.jsx",["156"],"import React, { useState } from \"react\";\nimport _ from \"lodash\";\nimport { itemAdded } from \"../../store/basket\";\n\nimport \"../../css/menu/menuModals.css\";\nimport { useDispatch } from \"react-redux\";\n\nfunction MenuModal({ itemDescription, itemName, itemPrice, onClose }) {\n  let [quantity, setQuantity] = useState(1);\n\n  const dispatch = useDispatch();\n\n  return (\n    <div className=\"menuModalContainer\">\n      <h1 className=\"menuModalInfo\">{itemName}</h1>\n      <p className=\"menuModalInfo\">{itemDescription}</p>\n      <p>£{itemPrice}</p>\n      <div className=\"counterContainer\">\n        <button\n          disabled={quantity <= 1}\n          className=\"counterButton minus\"\n          onClick={() => setQuantity((quantity -= 1))}\n        >\n          -\n        </button>\n        <div className=\"counter\">{quantity}</div>\n        <button\n          className=\"counterButton plus\"\n          onClick={() => setQuantity((quantity += 1))}\n        >\n          +\n        </button>\n      </div>\n      <button\n        className=\"addToBasketButton\"\n        onClick={() => {\n          dispatch(\n            itemAdded({\n              itemName: itemName,\n              quantity: quantity,\n              price: quantity * itemPrice,\n            })\n          );\n          onClose();\n        }}\n      >\n        <div className=\"addToBasketButtonInfo\">Add to order</div>\n        <div className=\"addToBasketButtonInfo\">£{itemPrice * quantity}</div>\n      </button>\n    </div>\n  );\n}\n\nexport default MenuModal;\n","/Users/josephtavinor/DengieTakeawayApp/DengieTakeawayApp/dengie-takeaway-app/src/components/menu/MenuModalOptions.jsx",[],"/Users/josephtavinor/DengieTakeawayApp/DengieTakeawayApp/dengie-takeaway-app/src/config/colors.js",["157"],"export default {\n  white: \"#fff\",\n};\n","/Users/josephtavinor/DengieTakeawayApp/DengieTakeawayApp/dengie-takeaway-app/src/components/homePage/CuisineList.jsx",["158"],"import React, { useEffect, useState } from \"react\";\nimport CuisineCard from \"./CuisineCard\";\nimport \"../../css/cuisineList.css\";\nimport { loadCuisines } from \"../../store/cuisines\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { Bars } from \"react-loading-icons\";\n\nfunction CuisineList() {\n  const [postcode, setPostcode] = useState(\"\");\n\n  const dispatch = useDispatch();\n  const cuisines = useSelector((state) => state.entities.cuisines.list);\n  const loading = useSelector((state) => state.entities.cuisines.loading);\n\n  useEffect(() => {\n    dispatch(loadCuisines());\n  }, []);\n\n  const renderCuisines = (cuisines) => {\n    const list = cuisines.map((cuisine) => (\n      <li className=\"cuisineItem\">\n        <CuisineCard cuisine={cuisine} p