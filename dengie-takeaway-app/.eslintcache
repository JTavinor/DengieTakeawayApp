ities.cuisines.list);\n  const loading = useSelector((state) => state.entities.cuisines.loading);\n  const error = useSelector((state) => state.entities.cuisines.error);\n\n  const filteredCuisines = filterCuisines(postcode, cuisines);\n\n  useEffect(() => {\n    dispatch(loadCuisines());\n  }, []);\n\n  return (\n    <React.Fragment>\n      {loading && <LoadingIcon />}\n      {!loading && !error && (\n        <React.Fragment>\n          <div className=\"cuisineContainer\">\n            <PostcodeChecker setPostcode={setPostcode} postcode={postcode} />\n            <CuisineList cuisines={filteredCuisines} />\n          </div>\n        </React.Fragment>\n      )}\n      {error && <ApiError error={error} />}\n    </React.Fragment>\n  );\n}\n\nexport default Hompage;\n",{"ruleId":"204","replacedBy":"205"},{"ruleId":"206","replacedBy":"207"},{"ruleId":"208","severity":1,"message":"209","line":2,"column":10,"nodeType":"210","messageId":"211","endLine":2,"endColumn":21},{"ruleId":"208","severity":1,"message":"212","line":5,"column":8,"nodeType":"210","messageId":"211","endLine":5,"endColumn":19},{"ruleId":"208","severity":1,"message":"213","line":9,"column":10,"nodeType":"210","messageId":"211","endLine":9,"endColumn":14},{"ruleId":"214","severity":1,"message":"215","line":20,"column":6,"nodeType":"216","endLine":20,"endColumn":8,"suggestions":"217"},{"ruleId":"208","severity":1,"message":"218","line":1,"column":17,"nodeType":"210","messageId":"211","endLine":1,"endColumn":25},{"ruleId":"208","severity":1,"message":"219","line":20,"column":9,"nodeType":"210","messageId":"211","endLine":20,"endColumn":14},{"ruleId":"208","severity":1,"message":"220","line":3,"column":8,"nodeType":"210","messageId":"211","endLine":3,"endColumn":9},{"ruleId":"208","severity":1,"message":"220","line":2,"column":8,"nodeType":"210","messageId":"211","endLine":2,"endColumn":9},{"ruleId":"214","severity":1,"message":"221","line":52,"column":6,"nodeType":"216","endLine":52,"endColumn":8,"suggestion